# API Configuration
# Base URL for backend API
VITE_API_BASE_URL=http://localhost:8081

# API request timeout in milliseconds (default: 10000 = 10 seconds)
VITE_API_TIMEOUT=10000

# Authentication Configuration
# JWT token expiration in days (default: 3)
VITE_TOKEN_EXPIRY_DAYS=3

# TanStack Query Configuration
# Auto-refetch interval in milliseconds (default: 60000 = 1 minute)
VITE_QUERY_REFETCH_INTERVAL=60000
